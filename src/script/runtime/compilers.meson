toolchain = import('toolchain')

toolchain.register(
    'slimcc',
    component: 'compiler',
    inherit: 'posix',
    default_linker: 'ld',
    detect: func(version_output str) -> bool
        return 'slimcc' in version_output
    endfunc,
)
